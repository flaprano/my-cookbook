<% if @cuisine.errors.any? %>
  <% @cuisine.errors.full_messages.each do |message| %>
  <div class="alert alert-danger" role="alert">
    <div class="row">
      <%= message %>
    </div>
  </div>
  <% end %>
<% end %>

<%= form_for @cuisine do |f| %>
  <%= f.label :name, 'Nome' %>
  <%= f.text_field :name %>
  <%= f.submit 'Enviar' %>
<% end %>
